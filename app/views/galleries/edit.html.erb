<%= form_tag "/galleries/#{@gallery.id}", method: :patch do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %><br>

  <%= label_tag :capacity %>
  <%= text_field_tag :capacity %><br>

  <%= label_tag :tech_support %>
  <%= hidden_field_tag :tech_support, false %>
  <%= check_box_tag :tech_support, true %><br>
  <%= submit_tag 'Submit' %>
<% end %>
